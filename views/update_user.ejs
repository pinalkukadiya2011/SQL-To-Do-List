<h1 align="cenetr">UPDATE THE USER</h1>
<form method="post"  >
    <table  align="center">

        <% results.forEach(function(user1) { %>
 
         <tr>
            <td>NAME</td>
            <td><input type="text" name="name" value="<%= user1.name%>" placeholder="ENTER THE NAME"/> </td>
        </tr> 
        <tr>
            <td>EMAIL</td>
            <td><input type="varchar" name="email" value="<%= user1.email%>" placeholder="ENTER THE EMAIL"/> </td>
        </tr> 
        <tr> 
            <td>PASSWORD</td>
                <td><input type="varchar" name="password" value="<%= user1.password%>" placeholder="ENTER THE PASSWORD"/> </td>
        </tr>
        <tr>
            <td></td>
            <td><input type="submit" name="submit"/> </td>
            
        </tr>
        <%  }) %>
    </table>
</form>